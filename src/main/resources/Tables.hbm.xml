<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name = "library.system.Tables" table = "tablesTable">
        <id name = "tablesNumber" column = "tablesNumber" type = "string" />
        <property name = "tablesSize" column = "tablesSize" type = "string"/>
        <property name = "tablesPlayers" column = "tablesPlayers" type = "string"/>
        <property name = "tablesQuantity" column = "tablesQuantity" type = "int"/>
    </class>
</hibernate-mapping>
